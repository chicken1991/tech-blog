{{#each posts as |post| }}
<div class="card" style="width: 50rem;">
  <div class="card-body">
    <h5 class="card-title">{{post.title}}</h5>
    <h6 class="card-subtitle mb-2 text-muted">Posted by YOU at {{format_date post.create_date}}</h6>
    <p class="card-text">{{post.text}}</p>
  </div>
</div>
{{/each}}